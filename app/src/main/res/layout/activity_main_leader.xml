<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_leader"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sabu.bongobondhu.MainLeaderActivity">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        tools:layout_editor_absoluteY="0dp"
        app:layout_constraintRight_toRightOf="@+id/activity_political">

    <ImageView
        android:layout_width="480dp"
        android:layout_height="320dp"
        app:srcCompat="@drawable/b6"
        android:layout_marginTop="18dp"
        android:id="@+id/imageView6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="@string/title_four"
        android:textSize="25dp"
        android:textColor="#1a1a1a"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:layout_marginTop="12dp"
        android:id="@+id/textView9"
        android:layout_below="@+id/imageView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/description_four"
        android:id="@+id/textView10"
        android:textSize="18dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textScaleX="1"
        android:textColor="#033012"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    </RelativeLayout>
</ScrollView>
